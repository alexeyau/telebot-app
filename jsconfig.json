{
  "compilerOptions": {
    "target": "es2017",
    "allowSyntheticDefaultImports": false,
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
      "@services/*": ["src/services/*"],
      "@telebots/*": ["src/telebots/*"],
      "@pages/*": ["src/pages/*"],
      "@components/*": ["src/telebots/*"]
    }
  },
  "exclude": ["node_modules", "dist", "build", "husky"]
}
